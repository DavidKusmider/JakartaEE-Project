<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
	  <servlet-name>ClientServletTest</servlet-name>
	  <servlet-class>ClientServletTest</servlet-class>
  </servlet>
  <servlet-mapping>
	  <servlet-name>ClientServletTest</servlet-name>
	  <url-pattern>/ClientServletTest</url-pattern>
  </servlet-mapping>


  <jsp-config>
	  <jsp-property-group>
		  <url-pattern>*.jsp</url-pattern><!-- inclut pour tous les fichier jsp  -->
		  <include-prelude>/WEB-INF/taglibs.jsp</include-prelude> <!--le fichier taglibs.jsp   -->
	  </jsp-property-group>
  </jsp-config>

</web-app>
